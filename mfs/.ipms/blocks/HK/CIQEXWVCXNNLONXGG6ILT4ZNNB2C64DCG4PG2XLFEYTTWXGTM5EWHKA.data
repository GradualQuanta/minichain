# dependencies:
release='QmfAFuDqWz2mLEZUmmjdM8zvs2QVYQXShe5orKGK4rMgZ6'
kwextract="/ipfs/$release/kwextract.pl"


# get mutable file from a block
file="$1"
mutable=$(ipfs cat $kwextract | perl /dev/stdin -k mutable $file)
if true; then
ipfs files read $mutable
else
echo "--- # \$mutable: $mutable\$"
ipfs files read $mutable | tail -1 | sed -e 's/^- /qm: /'
echo ...

fi

